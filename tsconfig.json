{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-mat-typeahead": [
        "dist/ngx-mat-typeahead/ngx-mat-typeahead",
        "dist/ngx-mat-typeahead"
      ],
      "ng7-common": [
        "dist/ng7-common/ng7-common",
        "dist/ng7-common"
      ],
      "iq-lib": [
        "dist/iq-lib/iq-lib",
        "dist/iq-lib"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}